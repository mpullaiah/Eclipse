package com.rishi.oops.constructor;

public class Employee extends Object {
	String name;
	int age;
	
	public Employee() {
		this.name ="rishi";
		this.age =23;
	}
	public Employee(String name) {
		this.name =name;
		this.age =22;
	}
	public Employee(int age) {
		this.name ="Rishi";
		this.age =22;
		}
	public Employee(String name,int age) {
		this.name =name;
		this.age=age;
	}
 }
