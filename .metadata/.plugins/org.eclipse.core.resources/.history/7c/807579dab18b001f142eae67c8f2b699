package com.rishi.demos;

public abstract class   Demo1 extends Demo implements InfiStandards{
	public abstract int add(int a,int b);
	public abstract int sub(int a,int b);

	public abstract int mul(int a,int b);

	public abstract int div(int a,int b);

	public abstract int rem(int a,int b);

	public abstract int power(int num,int exp);

	public abstract boolean checkPrime(int number);

	public abstract void fibonacciSeriesPrint(int untilln);

	public abstract boolean checkPalindrome(int number);
}
